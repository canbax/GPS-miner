{
  "name": "gps-miner",
  "version": "0.0.1",
  "description": "A script to scrape GPS data of cities and translates to different languages using wikidata",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "test": "vitest run",
    "test-dev": "vitest dev",
    "tsc": "tsc",
    "mine-GPS": "tsc && node src/mine-GPS.js && rm src/*.js",
    "i18n-1": "tsc && node src/i18n-1.js && rm src/*.js",
    "i18n-2": "tsc && node src/i18n-2.js && rm src/*.js",
    "del-js": "rm src/*.js"
  },
  "keywords": [
    "script",
    "GPS",
    "geo-location",
    "city",
    "country",
    "region"
  ],
  "author": "Yusuf Sait Canbaz",
  "license": "MIT",
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/node": "^20.11.19",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "cli-progress": "^3.12.0",
    "fast-csv": "^5.0.1",
    "wikibase-sdk": "^9.2.4"
  }
}